<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Checklist" preserve-order="true" parallel="tests" thread-count="4">
<listeners>
    <listener class-name="org.example.ECommerce.Utilities.Listeners.TakeScreenshot" />
    <listener class-name="org.example.ECommerce.Utilities.Listeners.TestNGCustomReportListener"></listener>

</listeners>
    <test name="Cart Module" preserve-order="true" >
        <classes>

            <class name="org.example.ECommerce.Tests.AddProductToCartTest"></class>
            <class name="org.example.ECommerce.Tests.IncreaseQuantityInCartTest"></class>
            <class name="org.example.ECommerce.Tests.AddProductFromFeaturedCollection"></class>
            <class name="org.example.ECommerce.Tests.AddProductOfDifferentSizeToCart"></class>

        </classes>
    </test>


</suite>
